{
  "name": "bambolee-digital/onboarding-package",
  "description": "Laravel/FilamentPHP package for managing customizable onboarding flows",
  "version": "1.0.2",
  "type": "library",
  "license": "MIT",
  "authors": [
      {
          "name": "Bambolee Digital",
          "email": "contact@bambolee.digital"
      }
  ],
  "require": {
      "php": "^8.1",
      "laravel/framework": "^11.0",
      "filament/filament": "^3.0",
      "spatie/laravel-translatable": "^6.8",
      "bambolee-digital/translatable-resource-kit": "^1.0",
    "filament/spatie-laravel-translatable-plugin": "^3.2"
  },
  "autoload": {
      "psr-4": {
          "BamboleeDigital\\OnboardingPackage\\": "src/"
      }
  },
  "extra": {
      "laravel": {
          "providers": [
              "BamboleeDigital\\OnboardingPackage\\Providers\\OnboardingServiceProvider"
          ]
      }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
